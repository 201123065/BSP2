<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>cargar_usuario</servlet-name>
        <servlet-class>servlets.cargar_usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>graficar</servlet-name>
        <servlet-class>servlets.graficar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>grafica_abb</servlet-name>
        <servlet-class>servlets.grafica_abb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>super_usuario</servlet-name>
        <servlet-class>servlets.super_usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessUpload</servlet-name>
        <servlet-class>servlets.ProcessUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_usuarios</servlet-name>
        <servlet-class>CSV.CSV_usuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_naves</servlet-name>
        <servlet-class>CSV.CSV_naves</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_juegos</servlet-name>
        <servlet-class>CSV.CSV_juegos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_partida</servlet-name>
        <servlet-class>CSV.CSV_partida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_historial</servlet-name>
        <servlet-class>CSV.CSV_historial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_contactos</servlet-name>
        <servlet-class>CSV.CSV_contactos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CSV_tiros</servlet-name>
        <servlet-class>CSV.CSV_tiros</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cargar_usuario</servlet-name>
        <url-pattern>/crear_usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>graficar</servlet-name>
        <url-pattern>/graficar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>grafica_abb</servlet-name>
        <url-pattern>/ver_abb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>super_usuario</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessUpload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_usuarios</servlet-name>
        <url-pattern>/usuario_csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_naves</servlet-name>
        <url-pattern>/CSV_naves</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_juegos</servlet-name>
        <url-pattern>/juego_csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_partida</servlet-name>
        <url-pattern>/partida_csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_historial</servlet-name>
        <url-pattern>/historial_csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_contactos</servlet-name>
        <url-pattern>/contactos_csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CSV_tiros</servlet-name>
        <url-pattern>/tiros_csv</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
